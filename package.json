{
  "name": "brandsync-design-system",
  "version": "1.0.0",
  "type": "module",
  "description": "Deterministic UI component library with layout primitives and pattern templates for compiler-driven UI generation",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tokens.css": "./styles/_tokens.css",
    "./components": "./components/index.json",
    "./patterns": "./patterns/index.json",
    "./contracts": "./contracts/validation.schema.json"
  },
  "files": [
    "dist",
    "components",
    "patterns",
    "contracts",
    "styles",
    "hooks"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build --mode library",
    "preview": "vite preview",
    "prepublishOnly": "npm run build:lib"
  },
  "keywords": [
    "react",
    "design-system",
    "ui-components",
    "layout-primitives",
    "design-tokens",
    "rag",
    "mcp",
    "compiler-driven-ui"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}
